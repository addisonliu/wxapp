
.container {
  background: #FFF;
  height: 100%;
  overflow: hidden;
  width: 100%;
  box-sizing: border-box;
  background-color: #f4f4f4;
  font-family: PingFangSC-Light,helvetica,'Heiti SC';
} 
.sell_cells {
  margin: 30rpx 0;
  padding: 0 30rpx;
  background-color: #fff;
}
 .titleinfo {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;

  height: 70rpx;
  background:#F8F8F8;
}

 .titleinfo .title {
  padding-right: 30rpx;
  background-size: 16.656rpx 27rpx;
  display: inline-block;
  height: 20rpx;
  font-size: 30rpx;
  line-height: 20rpx;
}

.out {
  margin-top: 20rpx;
}
.outDetail{
  width:520rpx;
  color: black !important;
}
.row {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 100rpx;
  background: white;
  color: #c1c1c1;
  border-bottom: 1px solid #c1c1c1;
  font-size: 32rpx;
}
.row view{
  padding:0 20rpx;
}
.textarea{
  width:60%;
  padding:0 20rpx;
  justify-content: right;
  font-size: 30rpx;
  text-align: right
}
.textColor{
  color:  black ;
  font-size: 15px;
  font-weight: bold;
}

.txt{
    float: right;
    font-size: 30rpx;
    color: #666;
    position: relative;
    align-items:center;
}
.butt{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100vw;
  height: 88rpx;
  line-height: 88rpx;
  margin-top: 20rpx;
  background: #2d9cf0;
  color: #fff;
  text-align: center;
  vertical-align: middle;
  touch-action: manipulation;
  cursor: pointer;
  white-space: nowrap;
  user-select: none;
  font-size: 14px;
  border: none;
  border-radius: 0;
  box-sizing: border-box;
  padding: 0 14px;
  text-decoration: none;
  z-index: 10;
}
.choose
{
  color:#f23030;
  text-align: right;
}
