.select_box{
  background: #fff;
  width: 60%;
  margin-left: 30rpx;
  position: relative;
}

.select{
  box-sizing: border-box;
  height: 80rpx;
  border:1px solid #efefef;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
}

.select_text{
  font-size: 30rpx;
  flex: 1;
}

.select_img{
  width: 40rpx;
  height: 40rpx;
  display: block;
  transition:transform 0.3s;
}

.select_img_rotate{
  transform:rotate(180deg); 
}

.option_box{
  position: absolute;
  top: 76rpx;
  width: 100%;
  border: 1px solid #efefef;
  box-sizing: border-box;
  height: 0;
  overflow-y: auto;
  border-top: 0;
  background: rgb(255, 255, 255);
  transition: height 0.3s;
}

.option{
  display: block;
  line-height: 40rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #efefef;
  padding: 10rpx;
}

.submit{
  position: fixed;
  width: 100%;
  height: 8%;
  bottom: 0;
  display: flex;
  vertical-align: middle;
  text-align: center;
  color: #fff;
  font-size: 16px;
}

.na{
  font-size: 30rpx;
  padding-right: 30rpx
} 

.mine_cells {
  margin: 20rpx 0;
  padding: 0 30rpx;
  background-color: #fff;
}

.mine_cell {
  display: flex;
  padding: 15rpx 0;
  align-items: center;
}

.mine_cell {
  border-bottom: 1px solid #e2e2e2;
}

.mine_icon {
  margin:0 20rpx;
  width: 50rpx;
  height: 50rpx;
}

.mine_title{
  font-size:16px;
  flex:1;
}

.mine_arrow{
  text-align: right;
}

/*伪元素实现右箭头*/
.mine_arrow:after{
  content: " ";
  display: inline-block;
  height:6px;
  width:6px;
  border-top:2px solid #b2b2b2; 
  border-right: 2px solid #b2b2b2;
  transform: rotate(45deg);
  margin-left: 5rpx;
}

.footer{
  position:absolute;
  bottom:10rpx;
  padding:0px;
  margin:0px
}

