.container {
  background: #FFF;
  height: 100%;
  overflow: hidden;
  width: 100%;
  box-sizing: border-box;
  background-color: #f4f4f4;
  font-family: PingFangSC-Light,helvetica,'Heiti SC';
} 

 .titleinfo {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  height: 70rpx;
  background:#F8F8F8;
}

 .titleinfo .title {
  padding-right: 30rpx;
  background-size: 16.656rpx 27rpx;
  display: inline-block;
  height: 20rpx;
  font-size: 30rpx;
  line-height: 20rpx;
}

.row {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 100rpx;
  background: white;
  color: #c1c1c1;
  border-bottom: 1px solid #c1c1c1;
  font-size: 32rpx;
}
.row view{
  padding:0 20rpx;
}
.textarea{
  width:60%;
  padding:0 20rpx;
  justify-content: right;
  font-size: 30rpx;
  text-align: right
}
.textColor{
  color:  black ;
  font-size: 15px;
  font-weight: bold;
}

.txt{
    float: right;
    font-size: 30rpx;
    color: #666;
    position: relative;
    align-items:center;
}
