.butt{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100vw;
  height: 88rpx;
  line-height: 88rpx;
  margin-top: 20rpx;
  background: #2d9cf0;
  color: #fff;
  text-align: center;
  vertical-align: middle;
  touch-action: manipulation;
  cursor: pointer;
  white-space: nowrap;
  user-select: none;
  font-size: 14px;
  border: none;
  border-radius: 0;
  box-sizing: border-box;
  padding: 0 14px;
  text-decoration: none;
  z-index: 10;
}
